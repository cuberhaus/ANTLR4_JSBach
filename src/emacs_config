;; Eval Buffer with `M-x eval-buffer' to register the newly created template.

(dap-register-debug-template
  "Python :: Run file (buffer)"
  (list :type "python"
        :args "input"
        :cwd "/home/pol/ANTLR4_JSBach/src/"
        :module nil
        :program nil
        :request "launch"
        :name "Python :: Run file (buffer)"))

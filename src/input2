PROVA 1
x <- 6
x + 2
y <- 12
x + y

PROVA 2
Main a b c |:
    <!> "Hallo Bach"
    Put
:|

Put |:
    a <- 2
    <!> a
:|

PROVA 3
Main |:
    x <- 2 = 2
    <!> "Main" x
    MainSec x 2+2
:|

MainSec a b |:
    <!> "This function has been called correctly! " a b
:|

PROVA 4

Main |:
    x <- 3
    if 1 > x |:
        <!> "If is working!"
    :| else |:
        <!> "else block!"
    :|
:|

PROVA 5 check if parenthesis work